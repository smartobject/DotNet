<Window x:Class="wpfRules.DialogBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DialogBox" Height="300" Width="300">
    <Grid>
        <Label x:Name="labelFieldOne" Content="Item Value:" HorizontalAlignment="Left" Height="26" Margin="10,34,0,0" VerticalAlignment="Top" Width="78"/>
        <TextBox x:Name="txtItem1" HorizontalAlignment="Left" Height="26" Margin="93,34,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="189"/>
        <Button x:Name="btnDialogOK" Content="Ok" HorizontalAlignment="Left" Height="22" Margin="211,224,0,0" VerticalAlignment="Top" Width="56" Click="btnDialogOK_Click"/>
        <Button x:Name="btnDialogCancel" Content="Cancel" HorizontalAlignment="Left" Height="22" Margin="10,224,0,0" VerticalAlignment="Top" Width="53" Click="btnDialogCancel_Click"/>

    </Grid>
</Window>
